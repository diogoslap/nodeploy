<%- include layout.ejs %>
	<form action="/projects/edit" method="POST">
	  <input type="hidden" value="<%= project.project_id %>" name="id"/>
	  <div class="form-group">
	    <label for="name">Project Name</label>
	    <input type="text" name="name" value="<%= project.name %>" class="form-control"  placeholder="Project Name">
	  </div>
	  <div class="form-group">
	    <label for="server">Project Server</label>
	    <input type="text" name="server" value="<%= project.server %>" class="form-control"  placeholder="Project Server">
	  </div>
	  <div class="form-group">
	    <label for="name">Project Command</label>
	    <input type="text" name="command" value="<%= project.command %>" class="form-control"  placeholder="Project Command">
	  </div>
	  <div class="form-group">
	    <label for="name">Project Command Test</label>
	    <input type="text" name="command_test" value="<%= project.command_test %>" class="form-control"  placeholder="Project Command">
	  </div>
	  <div class="form-group">
	    <label for="name">Project Command Rollback</label>
	    <input type="text" name="command_rollback" value="<%= project.command_rollback %>" class="form-control"  placeholder="Project Command">
	  </div>	  
	  <button type="submit" class="btn btn-default">Submit</button>
</form>
 
<%- include footer.ejs %>